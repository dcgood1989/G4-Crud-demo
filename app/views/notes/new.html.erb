<%=  form_for @note do |f| %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :capacity %>
  <%= f.number_field :capacity %>

  <%= f.label :name, "requires_id" %>
  <%= f.check_box :requires_id %>

  <%= f.submit "Create some fucking notes" %>

<% end %>
